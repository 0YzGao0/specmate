{
    "name": "specmate",
    "version": "0.0.1",
    "description": "Specmate for co-engineering requirements and tests",
    "scripts": {
        "clean": "clean.bat",
        "copy-bootstrap-css": "cpx node_modules/bootstrap/dist/css/bootstrap.min.css src/lib/css",
        "copy-bootstrap-js": "cpx node_modules/@ng-bootstrap/**/bundles/**/*.js src/lib/js/@ng-bootstrap",
        "copy-fontawsome-css": "cpx node_modules/font-awesome/css/font-awesome.min.css src/lib/css",
        "copy-fontawsome-fonts": "cpx node_modules/font-awesome/fonts/* src/lib/fonts",
        "copy-rxjs": "cpx node_modules/rxjs/**/*.js src/lib/js/rxjs",
        "copy-uuid": "cpx node_modules/angular2-uuid/**/*.js src/lib/js/angular2-uuid",
        "copy-angular": "cpx node_modules/@angular/**/bundles/**/*.umd.js src/lib/js/@angular",
        "copy-tslib": "cpx node_modules/tslib/tslib.js src/lib/js/tslib",
        "copy-shim": "cpx node_modules/core-js/client/shim.min.js src/lib/js",
        "copy-system": "cpx node_modules/systemjs/dist/system.src.js src/lib/js",
        "copy-zone": "cpx node_modules/zone.js/dist/zone.js src/lib/js",
        "copy-classlist": "cpx node_modules/classlist.js/classList* src/lib/js/classlist.js",
        "copy-angular-split": "cpx node_modules/angular-split/bundles/angular-split.umd.min.js src/lib/js/angular-split/",
        "copy-ng2-dragula": "cpx node_modules/ng2-dragula/bundles/ng2-dragula.umd.min.js src/lib/js/ng2-dragula/",
        "copy": "concurrently \"npm run copy-angular-split\" \"npm run copy-ng2-dragula\" \"npm run copy-uuid\" \"npm run copy-bootstrap-css\" \"npm run copy-bootstrap-js\" \"npm run copy-shim\" \"npm run copy-system\" \"npm run copy-zone\" \"npm run copy-angular\" \"npm run copy-rxjs\" \"npm run copy-fontawsome-css\" \"npm run copy-fontawsome-fonts\" \"npm run copy-classlist\" >nul",
        "build-copy": "npm run copy && npm run build",
        "build": "npm run lint && tsc -p src/",
        "build:watch": "tsc -p src/ -w",
        "serve": "lite-server -c=bs-config.json",
        "serve:e2e": "lite-server -c=bs-config.e2e.json",
        "prestart": "npm run build",
        "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
        "pree2e": "npm run build:e2e",
        "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
        "preprotractor": "webdriver-manager update",
        "protractor": "protractor protractor.config.js",
        "pretest": "npm run build",
        "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
        "pretest:once": "npm run build",
        "test:once": "karma start karma.conf.js --single-run",
        "lint": "tslint ./src/**/*.ts -t verbose --exclude ./src/app/model/**/*.ts",
        "dist": "dist.bat",
        "dep-graph": "ngd -p src/tsconfig.json",
        "pack": "webpack --config webpack/webpack.dev.js --verbose --profile --bail"
    },
    "keywords": [],
    "author": "Technical University of Munich - Maximilian Junker and Sebastian Eder",
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/common": "5.0.5",
        "@angular/compiler": "5.0.5",
        "@angular/core": "5.0.5",
        "@angular/forms": "5.0.5",
        "@angular/http": "5.0.5",
        "@angular/platform-browser": "5.0.5",
        "@angular/platform-browser-dynamic": "5.0.5",
        "@angular/router": "5.0.5",
        "systemjs": "0.20.19",
        "core-js": "2.5.1",
        "rxjs": "5.5.4",
        "zone.js": "0.8.18",
        "classlist.js": "1.1.20150312",
        "bootstrap": "4.0.0-alpha.6",
        "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.6",
        "font-awesome": "4.7.0",
        "angular2-uuid": "1.1.1",
        "angular-split": "1.0.0-rc.0",
        "ng2-dragula": "1.5.0"
    },
    "devDependencies": {
        "@types/node": "8.0.56",
        "concurrently": "3.5.1",
        "typescript": "2.6.2",
        "canonical-path": "0.0.2",
        "tslint": "5.8.0",
        "lodash": "4.17.4",
        "cpx": "1.5.0",
        "del-cli": "1.1.0",
        "rimraf": "2.6.2",
        "angular2-dependencies-graph": "1.0.0-alpha.12",
        "inflight": "1.0.6",
        "webpack": "3.10.0",
        "html-webpack-plugin": "2.30.1",
        "raw-loader": "0.5.1",
        "extract-text-webpack-plugin": "3.0.2",
        "webpack-merge": "4.1.1",
        "html-loader": "0.5.1",
        "awesome-typescript-loader": "3.4.1",
        "angular2-template-loader": "0.6.2",
        "file-loader": "1.1.5",
        "css-loader": "0.28.7",
        "style-loader": "0.19.0",
        "null-loader": "0.1.1",
        "exports-loader": "0.6.4",
        "font-awesome-webpack": "0.0.5-beta.2",
        "less": "2.7.3",
        "url-loader": "0.6.2"
    },
    "repository": {}
}