{
    "name": "specmate",
    "version": "0.0.1",
    "description": "Specmate for co-engineering requirements and tests",
    "scripts": {
        "clean": "rimraf ../bundles/specmate-ui-core/webcontent/*",
        "build": "npm run extract && npm run lint && npm run clean && npm run pack",
        "lint": "tslint ./src/**/*.ts -t verbose --exclude ./src/app/model/**/*.ts",
        "pack": "webpack --config webpack/webpack.config.js",
        "extract": "ngx-translate-extract -i ./src -o ./src/assets/i18n/de.json ./src/assets/i18n/gb.json --clean --sort --format namespaced-json",
        "dep-graph": "ngd -p src/tsconfig.json",
        "init": "npm prune && npm install && npm run build"
    },
    "keywords": [],
    "author": "Technical University of Munich - Maximilian Junker and Sebastian Eder",
    "license": "Apache-2.0",
