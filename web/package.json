{
        "name": "specmate",
        "version": "0.0.1",
        "description": "Specmate for co-engineering requirements and tests",
        "scripts": {
                "clean": "clean.bat",
                "copy-bootstrap-css": "cpx node_modules/bootstrap/dist/css/bootstrap.min.css src/lib/css",
                "copy-bootstrap-js": "cpx node_modules/@ng-bootstrap/**/bundles/**/*.js src/lib/js/@ng-bootstrap",
                "copy-fontawsome-css": "cpx node_modules/font-awesome/css/font-awesome.min.css src/lib/css",
                "copy-fontawsome-fonts": "cpx node_modules/font-awesome/fonts/* src/lib/fonts",
                "copy-rxjs": "cpx node_modules/rxjs/**/*.js src/lib/js/rxjs",
                "copy-uuid": "cpx node_modules/angular2-uuid/**/*.js src/lib/js/angular2-uuid",
                "copy-angular": "cpx node_modules/@angular/**/bundles/**/*.umd.js src/lib/js/@angular",
                "copy-shim": "cpx node_modules/core-js/client/shim.min.js src/lib/js",
                "copy-system": "cpx node_modules/systemjs/dist/system.src.js src/lib/js",
                "copy-zone": "cpx node_modules/zone.js/dist/zone.js src/lib/js",
                "copy-d3-deps": "cpx node_modules/d3*/**/*.js src/lib/js/d3",
                "copy-traceur": "cpx node_modules/traceur/bin/**/*.js src/lib/js/traceur",
                "copy-traceur-plugin": "cpx node_modules/systemjs-plugin-traceur/*.js src/lib/js/traceur",
                "copy-classlist": "cpx node_modules/classlist.js/classList* src/lib/js/classlist.js",
                "copy": "concurrently \"npm run copy-traceur\" \"npm run copy-uuid\" \"npm run copy-traceur-plugin\" \"npm run copy-bootstrap-css\" \"npm run copy-bootstrap-js\" \"npm run copy-shim\" \"npm run copy-system\" \"npm run copy-zone\" \"npm run copy-angular\" \"npm run copy-rxjs\" \"npm run copy-fontawsome-css\" \"npm run copy-fontawsome-fonts\" \"npm run copy-d3-deps\" \"npm run copy-classlist\" >nul",
                "build-copy": "npm run copy && tsc -p src/",
                "build": "tsc -p src/",
                "build:watch": "tsc -p src/ -w",
                "build:e2e": "tsc -p e2e/",
                "serve": "lite-server -c=bs-config.json",
                "serve:e2e": "lite-server -c=bs-config.e2e.json",
                "prestart": "npm run build",
                "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
                "pree2e": "npm run build:e2e",
                "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
                "preprotractor": "webdriver-manager update",
                "protractor": "protractor protractor.config.js",
                "pretest": "npm run build",
                "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
                "pretest:once": "npm run build",
                "test:once": "karma start karma.conf.js --single-run",
                "lint": "tslint ./src/**/*.ts -t verbose",
                "dist": "dist.bat"
        },
        "keywords": [],
        "author": "Technical University of Munich - Maximilian Junker and Sebastian Eder",
        "license": "Apache-2.0",
        "dependencies": {
                "@angular/common": "~4.2.6",
                "@angular/compiler": "~4.2.6",
                "@angular/core": "~4.2.6",
                "@angular/forms": "~4.2.6",
                "@angular/http": "~4.2.6",
                "@angular/platform-browser": "~4.2.6",
                "@angular/platform-browser-dynamic": "~4.2.6",
                "@angular/router": "~4.2.6",
                "systemjs": "0.19.40",
                "core-js": "~2.4.1",
                "rxjs": "~5.4.2",
                "zone.js": "~0.8.13",
                "classlist.js": "~1.1.20150312",
                "bootstrap": "4.0.0-alpha.6",
                "@ng-bootstrap/ng-bootstrap": "~1.0.0-alpha.28",
                "font-awesome": "~4.7.0",
                "traceur": "0.0.111",
                "systemjs-plugin-traceur": "0.0.3",
                "angular2-uuid": "1.1.1",
                "d3-ng2-service": "~1.15.0"
        },
        "devDependencies": {
                "@types/node": "^6.0.46",
                "concurrently": "~3.5.0",
                "typescript": "~2.4.1",
                "canonical-path": "~0.0.2",
                "tslint": "~5.5.0",
                "lodash": "~4.17.4",
                "cpx": "1.5.0",
                "del-cli": "0.2.1",
                "rimraf": "^2.5.4"
        },
        "repository": {}
}
